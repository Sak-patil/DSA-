#include <stdio.h>
#include<stdlib.h>
#include<ctype.h>
# define max 100
int stack[max];
int top=-1;
void push(int value){
    if(top==max-1){
        printf("overflow");
    }
    else{
        top = top + 1;
        stack[top]=value;
       }
}

int pop() {
    if (top == -1)
        printf("Stack Underflow\n");
    else
        return stack[top--];
    return 0;
}

int evaluation(char postfix[]){
    int i;
    for(i=0;postfix[i]!='\0';i++){
        char ch=postfix[i];
        if(isdigit(ch)){
            push(ch-'0');
        }
        else{
            int val1,val2,res;
            val1=pop();
            val2=pop();
            if(ch=='+'){
                res=val2+val1;
                
            }
            else if(ch=='-'){
                res=val2-val1;
                
            }
            else if(ch=='*'){
                res=val2*val1;
                
            }
            else if(ch=='/'){
                res=val2/val1;
                
            }
            else{
                return 0;
            }
            push(res);
        }
    }
    return pop();
}
int main(){
    char postfix[max];
    printf("expression\n");
    scanf("%s",postfix);
    int result=evaluation(postfix);
    printf("Result = %d\n", result);
    return 0;
}
